{
    "name": "@elieandraos/clockwork-vue",
    "description": "A Vue composable that offers methods to access the validation error bag in the template",
    "version": "1.0.4",
    "source": "src/index.js",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.esm.js",
    "scripts": {
        "info": "npm list -depth 0 && npm list -g --depth=0 && npm outdated || exit 0",
        "build": "rm -rf dist && node scripts/build.js",
        "test": "jest --verbose --coverage",
        "lint": "npx eslint src --fix",
        "format": "npx prettier --write .",
        "release": "node -r esm scripts/release.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elieandraos/clockwork-vue.git"
    },
    "bugs": {
        "url": "https://github.com/elieandraos/clockwork-vue/issues"
    },
    "homepage": "https://github.com/elieandraos/clockwork-vue#readme",
    "license": "ISC",
    "dependencies": {
        "@elieandraos/clockwork": "*",
        "vue": "^3.2.26"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.16.5",
        "@elieandraos/cli-tools": "*",
        "chalk": "^4.1.2",
        "esbuild": "^0.14.27",
        "esbuild-node-externals": "^1.4.1",
        "eslint": "^8.0.1",
        "esm": "^3.2.25",
        "jest": "^27.2.5",
        "prettier": "^2.5.1"
    }
}
